platform :ios, '<%= @ios_sdk_version %>'
supports_swift_versions '>= 4.2'
use_frameworks!
inhibit_all_warnings!

target '<%= @target_name %>' do
  <% if @google_ads %>
  pod 'Google-Mobile-Ads-SDK'
  <% end %>
  <% if @mintegral_ads %>
  pod 'MintegralAdSDK', "6.1.2.0"
  pod 'MintegralAdSDK/BannerAd', "6.1.2.0"
  pod 'MintegralAdSDK/InterstitialVideoAd', "6.1.2.0"
  pod 'MintegralAdSDK/InterstitialAd', "6.1.2.0"
  pod 'MintegralAdSDK/SplashAd', "6.1.2.0"
  <% end %>
  <% if @bytedance_ads %>
  pod 'Bytedance-UnionAD', "2.8.0.1"
  <% end %>
end
